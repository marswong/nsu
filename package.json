{
  "name": "nsu",
  "version": "0.1.0",
  "description": "Name Service Utilities",
  "scripts": {
    "build": "del-cli dist && tsc",
    "prepack": "npm run build",
    "test": "npm run build && ava"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marswong/nsu.git"
  },
  "keywords": [
    "domain",
    "name",
    "util"
  ],
  "author": "Mars Wong <marswong618@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marswong/nsu/issues"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "devDependencies": {
    "ava": "5.2.0",
    "del-cli": "5.0.0",
    "got": "12.6.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
